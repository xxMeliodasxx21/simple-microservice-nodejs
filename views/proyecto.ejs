<h1>Subir PDF</h1>

<form action="/subir-pdf" method="POST" enctype="multipart/form-data">
  <input type="file" name="pdf" accept="application/pdf" required>
  <button type="submit">Subir</button>
</form>

<% if (nombrePDF) { %>
  <h2>PDF subido:</h2>
  <a href="/pdf/<%= nombrePDF %>" target="_blank">Abrir PDF</a>
<% } %>
